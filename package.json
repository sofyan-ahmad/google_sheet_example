{
  "name": "google-sheet",
  "version": "0.0.0",
  "description": "Google Sheet Integration",
  "author": "sofyan.h.ahmad@gmail.com",
  "scripts": {
    "serve": "nodemon",
    "serve:prod": "yarn build && node dist/main.js",
    "build": "nest build",
    "build:deploy": "yarn build && sls deploy",
    "deploy": "sls deploy",
    "deploy:dev": "sls deploy --stage dev --env development",
    "deploy:staging": "sls deploy --stage staging --env staging",
    "deploy:prod": "sls deploy --stage prod --env production",
    "precommit": "yarn e2e:coverage",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "lint:fix": "yarn lint --fix",
    "e2e": "jest --config=jest-e2e.json --forceExit ",
    "e2e:watch": "jest --watch --config=jest-e2e.json ",
    "e2e:coverage": "jest --config=jest-e2e.json --coverage --coverageDirectory=./coverage "
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@nestjs/common": "^7.1.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.1.0",
    "@nestjs/platform-express": "^7.1.0",
    "@nestjs/swagger": "^4.5.8",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "express-handlebars": "^4.0.4",
    "googleapis": "^51.0.0",
    "hbs": "^4.1.1",
    "json-2-csv": "^3.7.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.26.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "snakecase-keys": "^3.2.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.1.0",
    "@types/cors": "^2.8.6",
    "@types/express-handlebars": "^3.1.0",
    "@types/hbs": "^4.0.1",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.153",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.5",
    "@types/supertest": "^2.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/parser": "^3.0.1",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  }
}
